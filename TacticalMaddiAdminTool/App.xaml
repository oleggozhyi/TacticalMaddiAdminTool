<Application x:Class="TacticalMaddiAdminTool.App"
             xmlns:local="clr-namespace:TacticalMaddiAdminTool"
             xmlns:converters="clr-namespace:TacticalMaddiAdminTool.Converters"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
            >
    <Application.Resources>
         
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <converters:ItemTypeToIconStyleConverter x:Key="itemTypeToIconStyleConverter" />
                    <local:Bootstrapper x:Key="bootstrapper" />
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
